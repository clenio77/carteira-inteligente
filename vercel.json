{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "version": 2,
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/apps/api/index.py"
        }
    ],
    "functions": {
        "apps/api/index.py": {
            "maxDuration": 30
        }
    }
}