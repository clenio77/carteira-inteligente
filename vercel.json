{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "version": 2,
    "framework": null,
    "functions": {
        "api/index.py": {
            "maxDuration": 30,
            "runtime": "python3.12"
        }
    },
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/api/index.py"
        }
    ]
}