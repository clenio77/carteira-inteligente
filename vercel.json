{
    "version": 2,
    "builds": [
        {
            "src": "apps/web/package.json",
            "use": "@vercel/next"
        },
        {
            "src": "apps/api/main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "apps/api/main.py"
        },
        {
            "src": "/(.*)",
            "dest": "/apps/web/$1"
        }
    ],
    "env": {
        "NEXT_PUBLIC_API_URL": "/api"
    }
}